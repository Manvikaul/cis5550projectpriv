var inputclass;
function cleary(inputclass, keytext){
	  $(inputclass).focus( function() {
		if ( $(inputclass).val()===keytext) {
            $(inputclass).val('');
        }  
	  });
	  $(inputclass).blur( function() {
        if ( $(inputclass).val()==='') {
            $(inputclass).val(keytext);
        } 
