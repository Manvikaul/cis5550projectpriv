const isPremiumHost = location.hostname.indexOf('premium') === 0;
const isMocks = location.pathname.startsWith('/mocks') || location.pathname.startsWith('/static-atlas/mocks');
const cookies = document.cookie.split(/;s*/);
let isPremiumUser = false;

for (let i = 0; i < cookies.length; i++) {
    if (cookies[i].indexOf('is_compass_premium=true') > -1) {
        isPremiumUser = true;
        break;
    }
}

if (isPremiumUser && !isPremiumHost && !isMocks) {
    if (location.hostname.indexOf('colleges') === 0) {
        location.href = location.href
            .replace('colleges', 'premium')
            .replace('rankingsandreviews.', '');
    } else if (location.hostname.indexOf('www') === 0) {
        location.href = location.href.replace('www', 'premium');
    }
} else if (isPremiumHost && !isPremiumUser && !isMocks) {
    location.href = location.href.replace('premium', 'www');
}</script><nav class="header__Container-sc-1geadoy-0 jtHA-Dx"><a href="https://web.archive.org/web/20240113091457/https://www.usnews.com/" class="header__LogoAnchor-sc-1geadoy-1 aBmO"><img src="/web/20240113091457im_/https://www.usnews.com/static-atlas/assets/img/usn-logo-large.svg" height="28" width="110" alt="U.S. News and World Report Logo"/></a><a class="header__LogoAnchor-sc-1geadoy-1 aBmO sm-hide" href="https://web.archive.org/web/20240113091457/https://www.usnews.com/about-us/90-years-of-USN"><img src="/web/20240113091457im_/https://www.usnews.com/static-atlas/assets/img/global/90th_logo.svg" height="33" width="50" alt="90th Anniversary Logo"/></a><div class="header__RootAnchorContainer-sc-1geadoy-5 cCokZL"><a href="https://web.archive.org/web/20240113091457/https://www.usnews.com/education" class="Anchor-byh49a-0 header__RootAnchor-sc-1geadoy-6 iKcQG dtsmDg">Education</a></div><div class="header__SwapContainer-sc-1geadoy-3 fGbzNM"><ul class="header__RootNavList-sc-1geadoy-4 hYkXSw"><li class="header__RootNavListItem-sc-1geadoy-9 iOxWLu"><a href="https://web.archive.org/web/20240113091457/https://www.usnews.com/best-colleges" data-tracking-campaign="Colleges" data-tracking-id="top_nav" class="header__RootNavAnchor-sc-1geadoy-8 inENeF">Colleges</a><div class="dropdown-menu__Container-sc-1jz3ypz-0 jzjtcv header__DropdownMenuStyled-sc-1geadoy-7 gFUobj mb2"><ul class="dropdown-menu__MenuList-sc-1jz3ypz-1 hCOIBl"><li><p class="Heading-sc-1w5xk2o-0 heading__MenuHeading-q3uk94-0 hSApDk xlfSv mx3 mb2 mt3"><a href="/web/20240113091457/https://www.usnews.com/best-colleges/rankings">Rankings and Directories</a></p><ul class="dropdown-menu__MenuList-sc-1jz3ypz-1 hCOIBl"><li><a href="https://web.archive.org/web/20240113091457/https://www.usnews.com/best-colleges/rankings/national-universities" class="item__ItemStyled-ji4pk4-0 jPUPMo">National Universities</a></li><li><a href="https://web.archive.org/web/20240113091457/https://www.usnews.com/best-colleges/rankings/national