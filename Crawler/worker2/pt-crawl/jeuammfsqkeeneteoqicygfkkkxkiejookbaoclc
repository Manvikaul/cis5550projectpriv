    <script type="text/javascript">
  getJSON('https://us-central1-web-services-dp.cloudfunctions.net/dropcap/34ST', function(json, textStatus) {
    const stories = json['result'];
    for (let i = 0; i < 4; i++) {
      if (i === 0) {
        document.getElementById('street-top-thumb').src = stories[i]['image'];
        document.getElementById('street-top-url').href = `https://34st.com${stories[i]['path']}`;
        document.getElementById('street-top-url').innerHTML = stories[i]['ogTitle'];
      } else {
        document.getElementById('popular-street').innerHTML +=
        `<a href="https://34st.com${stories[i]['path']}" class="frontpage-link small-link pub-link">${stories[i]['ogTitle']}</a>`;
      }
    }
  });
  getJSON('https://us-central1-web-services-dp.cloudfunctions.net/dropcap/UTB', function(json, textStatus) {
    const stories = json['result'];
    for (let i = 0; i < 4; i++) {
      if (i === 0) {
        document.getElementById('utb-top-thumb').src = stories[i]['image'];
        document.getElementById('utb-top-url').href = `https://underthebutton.com${stories[i]['path']}`;
        document.getElementById('utb-top-url').innerHTML = stories[i]['ogTitle'];
      } else {
        document.getElementById('popular-utb').innerHTML +=
        `<a href="https://underthebutton.com${stories[i]['path']}" class="frontpage-link small-link pub-link">${stories[i]['ogTitle']}</a>`;
      }
    }
  });
</script>
