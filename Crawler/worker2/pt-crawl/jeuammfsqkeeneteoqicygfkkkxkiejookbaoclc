<script type="text/javascript">
  getJSON('https://us-central1-web-services-dp.cloudfunctions.net/dropcap/DP', function(json, textStatus) {
    var stories = json['result'];
    for (var story in stories) {
      if (story < 3) {
        var storyIndex = parseInt(story);
        document.getElementById('mostRead').innerHTML +=
          "<div class='row'>" +
            "<div class='col-sm-1 count-col'>" +
              "<span class='count'>" + (storyIndex+1) +"</span>" +
            "</div>" +
            "<div class='col-sm-5 most-read-item'>" +
              "<div class='top-thumbnail'>" +
                "<img src='" + stories[storyIndex]['image'] + "' class='img' />" +
              "</div>" +
              "<a href=" + stories[storyIndex]['path'] + " class='frontpage-link standard-link'>" + stories[storyIndex]['ogTitle'] + "</a>" +
            "</div>" +

            "<div class='col-sm-1 count-col'>" +
              "<span class='count'>" + (storyIndex+4) +"</span>" +
            "</div>" +
            "<div class='col-sm-5 most-read-item'>" +
              "<div class='top-thumbnail'>" +
                "<img src='" + stories[storyIndex+3]['image'] + "' class='img' />" +
              "</div>" +
              "<a href=" + stories[storyIndex+3]['path'] + " class='frontpage-link standard-link'>" + stories[storyIndex+3]['ogTitle'] + "</a>" +
            "</div>" +
          "</div>"
        }
      }
  });
</script>

