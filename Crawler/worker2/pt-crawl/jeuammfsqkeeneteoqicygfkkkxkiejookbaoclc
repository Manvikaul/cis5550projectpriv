      <script type="text/javascript">
        function AdBlockEnabled() {
          var ad = document.createElement('ins');
          ad.className = 'AdSense';
          ad.style.display = 'block';
          ad.style.position = 'absolute';
          ad.style.top = '-1px';
          ad.style.height = '1px';
          document.body.appendChild(ad);
          var isAdBlockEnabled = !ad.clientHeight;
          document.body.removeChild(ad);
          return isAdBlockEnabled;
        }
        var closeAdbar = function () {
          document.getElementById('adSidebar').setAttribute("style", 'right:-350px;')
          document.getElementById('content').setAttribute("style", 'right:0;')
        }

        var openAdBar = function () {
          document.getElementById('adSidebar').setAttribute("style", 'right:0;')
          document.getElementById('content').setAttribute("style", 'right:16%;')
        }

        document.addEventListener("DOMContentLoaded", function (event) {
          if (!AdBlockEnabled()) {
            openAdBar();
          } else {
            closeAdbar();
          }
        });
      </script>
    
