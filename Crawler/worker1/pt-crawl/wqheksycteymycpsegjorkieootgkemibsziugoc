<script src='https://www.loc.gov/pictures/static/plugins/jquery.carouFredSel-5.3.0.js'></script>
<script type="text/javascript"> 
jQuery.fn.autocomplete = null;
jQuery(function($) {
    $( "#tabs_carous" ).tabs();
    var featured = $( "#featured" ).show().html(),
        topical = $( "#topical" ).show().html(),
        startfeatured = function(){
            $('#featured-carousel').carouFredSel({
                width: 625,
                height: 145,
                prev: '#featured-prev',
                next: '#featured-next',
                auto: true,
                scroll: {
                    pauseOnHover: true,
                    duration: 1000
                },
                pagination: {
                    container: "#featured-pager"
                },
                circular: true
            });
        },
        starttopical = function(){
        
            $('#topical-carousel').carouFredSel({
                width: 260,
                height: 145,
                prev: '#topical-prev',
                next: '#topical-next',
                auto: true,
                scroll: {
                    pauseOnHover: true
                },
                pagination: {
                    container: "#topical-pager"
                },
                circular: true
            });
            
        };
        
        
    startfeatured();
    
    $('a[href="#topical"]').bind('click',function(){
        
        starttopical();
    });
    $('a[href="#featured"]').bind('click',function(){
        
        startfeatured();
    });
        
});
</script> 

		
