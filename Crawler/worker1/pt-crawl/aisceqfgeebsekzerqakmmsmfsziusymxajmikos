<script type="text/javascript">
  getJSON('https://us-central1-web-services-dp.cloudfunctions.net/dropcap/DP', function(json, textStatus) {
    var stories = json['result'];
    for (var story in stories) {
      var number = parseInt(story) + 1;
      if (story < 3) {
        document.getElementById('popular').innerHTML +=
        "<div class='row'>" +
          "<div class='col-xs-2 num'>" +
            number +
          "<\/div>" +
          "<div class='col-xs-10'>" +
            "<a href=" + stories[story]['path'] + "><img src='" + stories[story]['image'] + "' class='img img-responsive' />"+ "<\/a>"  +
            "<a href=" + stories[story]['path'] + " class='frontpage-link standard-link'>" + stories[story]['ogTitle'] + "<\/a>" +
          "<\/div>" +
        "<\/div>"
      } else {
        document.getElementById('popular').innerHTML +=
        "<div class='row'>" +
          "<div class='col-xs-2 num'>" +
            number +
          "<\/div>" +
          "<div class='col-xs-10 most-read-item'>" +
            "<a href=" + stories[story]['path'] + " class='frontpage-link standard-link'>" + stories[story]['ogTitle'] + "<\/a>" +
          "<\/div>" +
        "<\/div>"
      }
    }
  });
</script>
