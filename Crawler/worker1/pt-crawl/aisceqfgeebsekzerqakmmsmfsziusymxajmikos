    getJSON('https://us-central1-web-services-dp.cloudfunctions.net/dropcap/DP', function(json, textStatus) {
      var stories = json['result'];
      for (var story in stories) {
        var number = parseInt(story) + 1;
        if (story < 3) {
          document.getElementById('popularMobile').innerHTML +=
            "<div class='col-sm-4 related'>" +
              "<div class='related-img'>" +
                "<a href=" + stories[story]['path'] + "><img src='" + stories[story]['image'] + "' class='img img-responsive' />"+ "<\/a>"  +
              "<\/div>" +
              "<h5><a href=" + stories[story]['path'] + " class='small-link'>" + stories[story]['ogTitle'] + "<\/a><\/h5>" +
            "<\/div>"
        }
      }
    });
  </script>
